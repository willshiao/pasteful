{
  "name": "pasteful",
  "version": "1.0.0",
  "description": "A feature-rich pastebin site written in Node.js using MongoDB and Redis.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "node ./node_modules/cross-env/bin/cross-env.js NODE_ENV=test node ./node_modules/mocha/bin/mocha",
    "travis-test": "node ./node_modules/cross-env/bin/cross-env.js NODE_ENV=test node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha"
  },
  "keywords": [
    "pastebin",
    "restify",
    "mongoDB",
    "redis"
  ],
  "author": "William Shiao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willshiao/pasteful.git"
  },
  "bugs": {
    "url": "https://github.com/willshiao/pasteful/issues"
  },
  "homepage": "https://github.com/willshiao/pasteful#readme",
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "config": "^1.21.0",
    "express": "^4.14.0",
    "mongoose": "^4.5.1",
    "redis": "^2.6.2",
    "shortid": "^2.2.6",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.1.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chance": "^1.0.4",
    "cross-env": "^2.0.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "mocha-mongoose": "^1.2.0",
    "mongodb": "^2.2.4"
  }
